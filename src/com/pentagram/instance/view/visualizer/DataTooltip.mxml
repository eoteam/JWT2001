<?xml version="1.0" encoding="utf-8"?>
<s:SkinnableContainer xmlns:fx="http://ns.adobe.com/mxml/2009" backgroundAlpha="1" backgroundColor="#242424" 
					  xmlns:s="library://ns.adobe.com/flex/spark" skinClass="com.pentagram.view.skins.components.DataTooltipSkin" 
					  xmlns:mx="library://ns.adobe.com/flex/mx" width="180" height="100" xmlns:vo="com.pentagram.model.vo.*">
	
	<fx:Script>
		<![CDATA[

		]]>
	</fx:Script>
	<fx:Declarations>
		<vo:DataRow id="row" />
	</fx:Declarations>
	<s:layout>
		<s:VerticalLayout  paddingLeft="4" paddingRight="4" paddingTop="4" paddingBottom="4"/>
	</s:layout>
	<s:HGroup left="0" right="0">
		<s:Label  color="#ffffff" fontFamily="FlamaBold" id="country" text="{row.country.name}" />	
		
	</s:HGroup>
	
	
	
	<s:Line width="100%" height="0.5">
		<s:stroke>
			<s:SolidColorStroke weight="0.25" color="0xB3B1AC" />
		</s:stroke>
	</s:Line>
</s:SkinnableContainer>
