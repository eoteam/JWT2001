<?xml version="1.0" encoding="utf-8"?>
<s:MXDataGridItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
						  xmlns:s="library://ns.adobe.com/flex/spark" 
						  xmlns:mx="library://ns.adobe.com/flex/mx" 
						  focusEnabled="false" autoDrawBackground="false">
	
	<fx:Script>
		<![CDATA[
			[Bindable] public var value:*;
			override public function set data(value:Object):void 
			{  
				super.data = value;    
				if(value != null)  
				{
					this.value = value.points[dataGridListData.dataField];
				}   
			} 
		]]>
	</fx:Script>
	<s:Label id="lblData" top="0" left="0" right="0" bottom="0" textAlign="left" paddingLeft="2" text="{value}" />
	
</s:MXDataGridItemRenderer>
