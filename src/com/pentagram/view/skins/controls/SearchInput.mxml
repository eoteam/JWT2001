<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark" xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:d="http://ns.adobe.com/fxg/2008/dt" 
		xmlns:flm="http://ns.adobe.com/flame/2008">
	
	<fx:Metadata>[HostComponent("spark.components.TextInput")]</fx:Metadata>

	<s:states>
		<s:State name="normal"/>
		<s:State name="disabled" stateGroups="disabledStates"/>
		<s:State name="normalWithPrompt" />
		<s:State name="disabledWithPrompt" stateGroups="disabledStates"/>
	</s:states>
	

	<s:Group id="group23" d:id="3" flm:knockout="false" left="0" right="0" y="1" height="24">
		<s:Rect  radiusX="1.9919" radiusY="1.9919" left="0" right="0" top="0" bottom="0">
			<s:fill>
				<s:SolidColor color="#F2F2F2"/>
			</s:fill>
		</s:Rect>
	</s:Group>
	
	<s:Rect height="24" id="rect20" radiusX="1.9919" radiusY="1.9919" left="0" right="0" x="0" y="0">
		<s:fill>
			<s:SolidColor color="#CCCCCC"/>
		</s:fill>
	</s:Rect>
	
	<s:Group id="group24" d:id="5" flm:knockout="false" left="1" right="1" height="22">
		<s:Rect  radiusX="1.9919" radiusY="1.9919" left="0" right="0" top="0" bottom="0">
			<s:fill>
				<s:LinearGradient rotation="270" scaleX="10.334" x="110.492" y="10.167">
					<s:GradientEntry color="#FFFFFF" ratio="0"/>
					<s:GradientEntry color="#E6E6E6" ratio="1"/>
				</s:LinearGradient>
			</s:fill>
		</s:Rect>
	</s:Group>

	
	<s:RichEditableText color="#333333"  left="8" right="8" top="4" bottom="4"
	fontSize="13" id="textDisplay" tabStops="S0 S50 S100 S150"  heightInLines="1"/>
	
	
	
	<s:Label id="promptDisplay" left="4" right="4" top="1" bottom="0" includeIn="normalWithPrompt"
			 color="#aaaaaa" fontFamily="FlamaBookItalic" fontStyle="normal"
			 maxDisplayedLines="1"
			 verticalAlign="middle" />
</s:Skin>
