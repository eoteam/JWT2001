<?xml version="1.0" encoding="utf-8"?>
<!---
Default skin for IntervalSlider
middleTrack,thumb1,thumb2 components have their includeinLayout
property set to false,
thumb1,thumb2 have their x coordinate set to false by IntervalSlider.as
Due to a bug in flex you must specify the width and height of these
components.
-->
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Metadata>
		[HostComponent("com.pentagram.view.controls.IntervalSlider")]
	</fx:Metadata>
	
	<s:Button  x="0" y="4" skinClass="com.pentagram.view.skins.controls.DarkSliderTrack"
	 id="middleTrack" />

	<s:Button id="thumb1" skinClass="com.pentagram.view.skins.controls.DarkSliderThumb" 
			  label="{hostComponent.pendingT1Val}" alpha="0.8"/>
	
	<s:Button id="thumb2" skinClass="com.pentagram.view.skins.controls.DarkSliderThumb" 
			  label="{hostComponent.pendingT2Val}" alpha="0.8"/>
	
</s:Skin>